<?xml version="1.0" encoding="utf-8"?>
<layer-list
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://schemas.android.com/apk/res/android ">
    <item>
        <shape android:shape="rectangle">
            <corners android:radius="0.0dip"/>
            <solid android:color="@color/blue_divider"/>
        </shape>
    </item>
    <item android:bottom="2.0dip">
        <shape android:shape="rectangle">
            <corners android:bottomLeftRadius="0.0dip" android:bottomRightRadius="0.0dip"/>
            <solid android:color="@color/blue_unselected"/>

        </shape>
    </item>
</layer-list>